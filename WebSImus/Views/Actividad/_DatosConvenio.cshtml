@model SM.LibreriaComun.DTO.EntidadesOperadoras.ConvenioDTO

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="panel-heading">
        <div class="row">
            <div class="col-md-2">
                <h3> Convenio #</h3>
            </div>
            <div class="col-md-5">
                <h3>  @Html.DisplayFor(model => model.Nombre)</h3>
            </div>
        </div>

        <div class="row">
            <div class="col-md-2">
                <h3> Entidad</h3>
            </div>
            <div class="col-md-5">
                <h3>  @Html.DisplayFor(model => model.NombreEntidad)</h3>
            </div>
        </div>
    </div>

}