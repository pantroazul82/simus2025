@model WebSImus.Models.EscuelasPadre
@{
    ViewBag.Title = "Ficha Escuela";
    Layout = "~/Views/Shared/_LayoutPublico.cshtml";
}
<style type="text/css">
    #PanelMapa {
        height: 300px;
    }

    #htab2 {
        display: block;
    }
</style>
<div class="row">
    <!-- ficha escuela -->
    <div class="col-md-12">
        <div class="col-md-2 col-sm-5 col-xs-5"><img style="padding-top:10px; padding-left:30px;" class="img-responsive" src="~/img/iconos/escuelas_big.png"></div>
        <div class="titulo_seccion col-md-10 col-sm-7 col-xs-7">ESCUELAS</div>

    </div>
</div>
<div class="row">
    <div class="subtitulo_seccion col-md-6 col-sm-12 col-xs-12 "><span class="fa-escuelas fa-lg" style="text-align: left; padding: 5px;"></span> @Html.DisplayFor(m => m.Escuelas.NombreEscuela)</div>
</div>

<div class="col-md-12 well carousel-search" style="margin-top:40px;">
    <div class="row">

        <a href="#" class="nav-tabs-dropdown btn btn-block btn-primary">Ficha escuelas</a>
        <ul style="padding-left:30px;" id="nav-tabs-wrapper" class="nav nav-tabs nav-tabs-horizontal">
            <li id="list1" class="active"><a href="#htab1" data-toggle="tab">Información básica</a></li>
            <li id="list2"><a href="#htab2" data-toggle="tab">Beneficiarios (Alumnos)</a></li>
            <li id="list3"><a href="#htab3" data-toggle="tab">Docentes</a></li>
            <li id="list4"><a href="#htab4" data-toggle="tab">Infraestructura y dotación</a></li>
            <li id="list5"><a href="#htab5" data-toggle="tab">Formación</a></li>
            <li id="list6"><a href="#htab6" data-toggle="tab">Producción</a></li>

        </ul>
        <div class="tab-content">

            <div role="tabpanel" class="tab-pane fade in active" id="htab1">
                <h3 class="titulo_ficha this-animate" data-animate="fadeInLeft bounce">Información básica </h3>

                <div class="divider"></div>
                <div class="page-content-wrap">
                    <div class="divider"></div>

                    <!-- page content holder -->
                    <div class="page-content-holder">

                        <div class="row">
                            <div class=" this-animate" data-animate="fadeInRight bounce">
                                <div class="col-md-8">
                                    <div class="modal-content">

                                        <div class="modal-header">
                                            <div class="row">

                                                <div class=" col-md-5 col-sm-5 col-xs-12 imagen_ficha">
                                                    <img style="border-radius: 5px 0px 0px 0px;" src="@Url.Content(ViewBag.ImageData)" class="img-responsive" alt="escuela" />
                                                    <div class="titulo_negro_ficha col-md-12">

                                                        <div class=" col-md-3 col-sm-2 col-xs-2 fa-escuelas fa-2x">  </div>
                                                        <div class="col-md-9   texto_foto"> @Html.DisplayFor(m => m.Institucionalidad.NombreDirector) </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-7 col-sm-7 col-xs-12">
                                                    <div class="col-md-12 titulo_ficha_item">  @Html.DisplayFor(m => m.Escuelas.NombreEscuela)  </div>
                                                    <div class="col-md-10 parrafo_ficha">@Html.DisplayFor(m => m.Escuelas.Resena)</div>
                                                </div>


                                            </div>

                                        </div>

                                    </div>
                                </div>
                                <div class=" this-animate" data-animate="fadeInDown bounce">
                                    <div class="col-md-3 redes_ficha">

                                        <div class="col-md-12 col-sm-3 col-xs-6">
                                            <div class="col-md-2 fa-mapas fa-3x iconos_ficha"> </div>
                                            <div class="col-md-10 ubicacion_fichas">@Html.DisplayFor(m => m.Escuelas.Direccion) @Html.DisplayFor(m => m.Escuelas.ZonaGeografica) </div>
                                            @Html.HiddenFor(model => model.Escuelas.Latitud)
                                            @Html.HiddenFor(model => model.Escuelas.Longitud)
                                        </div>
                                        <div class="col-md-12 col-sm-3 col-xs-6">
                                            <div class="col-md-2 fa-celular fa-3x iconos_ficha"> </div>
                                            <div class="col-md-10 txt_fichas">@Html.DisplayFor(m => m.Escuelas.Telefono) </div>
                                        </div>

                                        @if (!Model.Escuelas.SitioWeb.IsEmpty())
                                        {
                                            <div class="col-md-12 col-sm-3 col-xs-6">
                                                <div class="col-md-2 fa-web fa-3x iconos_ficha"> </div>
                                                <div class="col-md-10  txt_fichas"><a href="@Model.Escuelas.SitioWeb" target="_blank"> @Model.Escuelas.SitioWeb</a></div>
                                            </div>
                                        }
                                        <div class="col-md-12 col-sm-3 col-xs-6">
                                            <div class="col-md-2 fa-correo fa-3x iconos_ficha"> </div>
                                            <div class="col-md-10 txt_fichas">@Html.DisplayFor(m => m.Escuelas.CorreoElectronicoEscuela) </div>
                                        </div>

                                        <div class="col-md-12 col-sd-12 col-xs-12" style="margin-top:30px;">
                                            @if (Model.RedesSociales.Facebook.IsEmpty())
                                            { <div class="col-md-3 col-sd-3 col-xs-3 fa-face fa-3x red_desab"></div>}
                                            else
                                            {
                                                <div class="col-md-3 col-sd-3 col-xs-3"><a href="@Model.RedesSociales.Facebook" target="_blank"><span class="fa-face fa-3x iconos_ficha"></span></a></div>}
                                            @if (Model.RedesSociales.CanalYoutube.IsEmpty())
                                            { <div class="col-md-3 col-sd-3 col-xs-3 fa-youtube2 fa-3x red_desab"></div> }
                                            else
                                            {
                                                <div class="col-md-3 col-sd-3 col-xs-3"><a href="@Model.RedesSociales.CanalYoutube" target="_blank"><span class="fa-youtube2 fa-3x iconos_ficha"></span></a></div>
                                            }

                                            @if (Model.RedesSociales.Twitter.IsEmpty())
                                            {  <div class="col-md-3 col-sd-3 col-xs-3 fa-twitter2 fa-3x red_desab"></div>}
                                            else
                                            {
                                                <div class="col-md-3 col-sd-3 col-xs-3"><a href="@Model.RedesSociales.Twitter" target="_blank"><span class="fa-twitter2 fa-3x iconos_ficha"></span></a></div>
                                            }
                                            @if (Model.RedesSociales.GaleriaFlicker.IsEmpty())
                                            { <div class="col-md-3 col-sd-3 col-xs-3 fa-flickr2 fa-3x red_desab"></div> }
                                            else
                                            {
                                                <div class="col-md-3 col-sd-3 col-xs-3"><a href="@Model.RedesSociales.GaleriaFlicker" target="_blank"><span class="fa-flickr2 fa-3x iconos_ficha"></span></a></div>
                                            }
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                        <div class="row" style=" text-align:center;">
                            <div class="col-md-12 col-sd-12 col-xs-12" style="margin-top:20px;">

                                <div class="col-md-12 titulo_ficha">Ubicación </div>
                            </div>

                            <div class="col-md-12 col-sd-12 col-xs-12 ubicacion_fichas" style="margin-top:30px;">
                                <div id="PanelMapa" class="col-md-12">
                                </div>
                            </div>
                        </div>

                        <!-- Inicia vídeos -->

                        <div class="row" style=" text-align:center;">
                            @if (Model.listadoVideo.Count() > 0)
                            {
                                <div class="col-md-12 col-sd-12 col-xs-12" style="margin-top:20px;">

                                    <div class="col-md-12 titulo_ficha">Videos </div>
                                </div>
                                <div class="col-md-10 this-animate" data-animate="fadeInRight bounce">

                                    <div id="carousel-example-generic" class="carousel slide">
                                        <!-- Indicators -->
                                        <ol class="carousel-indicators">

                                            @for (int i = 0; i < Model.listadoVideo.Count(); i = i + 1)
                                            {
                                                if (i == 0)
                                                {
                                                    <li data-target="#carousel-example-generic" data-slide-to=@i class="active"></li>
                                                }
                                                else
                                                {
                                                    <li data-target="#carousel-example-generic" data-slide-to=@i></li>
                                                }

                                            }

                                        </ol>



                                        <!-- Wrapper for slides -->
                                        <div class="carousel-inner">
                                            @{int tem = 0;
                                            foreach (WebSImus.Models.VideoModel item in Model.listadoVideo)
                                            {
                                                if (tem == 0)
                                                {
                                                    <div class="item active">
                                                        <div class="titulo_video">  @item.NuevaClasificacion </div>
                                                        <div class="vid">
                                                            <iframe width="560" height="315" src=@item.url allowfullscreen=""></iframe>

                                                        </div><!--./vid -->
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="item">
                                                        <div class="titulo_video"> @item.NuevaClasificacion </div>
                                                        <div class="vid">
                                                            <iframe width="560" height="315" src=@item.url allowfullscreen=""></iframe>

                                                        </div>
                                                    </div>
                                                }
                                                tem = 1;
                                            }

                                            }
                                        </div>
                                    </div>

                                </div>
                            }
                        </div>
                        <!-- fin vídeos -->
                    </div>

                    <!-- ./2 -->
                </div>

            </div>
            <div role="tabpanel" class="tab-pane fadeIn" id="htab2">
                <div class=" col-md-1">
                    <img class=" icon_ficha this-animate" data-animate="fadeInLeft bounce" src="~/img/iconos/alumnos_big.png">
                </div>
                <div class=" col-md-11">

                    <h3 class="titulo_ficha this-animate" data-animate="fadeInLeft bounce">Beneficiarios (Alumnos)</h3>
                </div>

                <div class="page-content-wrap ">
                    <div class="divider"></div>

                    <!-- page content holder -->
                    <div class="page-content-holder">

                        <div class="row">
                            <div class=" this-animate" data-animate="fadeInRight bounce">
                                <div class="col-md-6">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <div class="row">
                                                <div class=" col-md-12 titulo_alumnos">Alumnos por rango de edad </div>
                                                <div class="row">

                                                    <div class="panel panel-default">
                                                        <div class="panel-body padding-0">
