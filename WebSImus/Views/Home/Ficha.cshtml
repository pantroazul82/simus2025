                    <img class=" this-animate icon_ficha" data-animate="fadeInLeft bounce" src="~/img/iconos/infra_big.png">
                </div>
                <div class=" col-md-11">

                    <h3 class="titulo_ficha this-animate" data-animate="fadeInLeft bounce">Infraestructura y dotación</h3>
                </div>
                <div class="page-content-wrap ">
                    <div class="divider"></div>

                    <!-- page content holder -->
                    <div class="page-content-holder">

                        <div class="row">
                            <div class=" this-animate" data-animate="fadeInRight bounce">

                                <div class="col-md-6">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <div class="row">
