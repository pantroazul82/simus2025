                                        </div>
                                    </div>
                                </div>
                                <!-- ./fin box 1 -->
                                <div class="col-md-6">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <div class="row">
                                                <div class=" col-md-12 titulo_alumnos2">Alumnos por etnia </div>
                                                <div class="row">

                                                    <div class="panel panel-default">
                                                        <div class="panel-body padding-0">
                                                            <div class="panel-body padding-0">
                                                                <div class="col-md-12 col-sm-12 col-xs-12">
                                                                    <div id="chartContaineretnia" style="height: 300px; width: 100%;"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!-- ./fin box 2 -->
                                <!-- ./fin box 3-->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-md-6" style="margin-top:20px;">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <div class="row">
                                                        <div class=" col-md-12 titulo_alumnos2">Alumnos por área</div>
                                                        <div class="row">
                                                            <div class="panel panel-default">
                                                                <div class="panel-body padding-0">
                                                                    <div class="panel-body padding-0">
                                                                        <div class="col-md-12 col-sm-12 col-xs-12">
                                                                            <div id="chartContainerubicacion" style="height: 300px; width: 100%;"></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-md-6" style="margin-top:20px;">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <div class="row">
                                                        <div class=" col-md-12 titulo_alumnos">Alumnos por otras condiciones</div>
                                                        <div class="row">
                                                            <div class="panel panel-default">
                                                                <div class="panel-body padding-0">
                                                                    <div class="panel-body padding-0">
                                                                        <div class="col-md-12 col-sm-12 col-xs-12">
                                                                            <div id="chartContainercondiciones" style="height: 300px; width: 100%;"></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <!-- ./empieza nuevamente-->
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-md-6">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <div class="row">
                                                        <div class=" col-md-12 titulo_alumnos">Alumnos por sexo</div>
                                                        <div class="row">
                                                            <div class="panel panel-default">
                                                                <div class="panel-body padding-0">
                                                                    <div class="panel-body padding-0">
                                                                        <div class="col-md-12 col-sm-12 col-xs-12">
                                                                            <div id="chartContainersexo" style="height: 300px; width: 100%;"></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ./page content holder -->
                </div>
            </div>

            <div role="tabpanel" class="tab-pane fade" id="htab3">
                <div class=" col-md-1">
                    <img class=" icon_ficha this-animate" data-animate="fadeInLeft bounce" src="~/img/iconos/docentes_big.png">
                </div>
                <div class=" col-md-11">

                    <h3 class="titulo_ficha this-animate" data-animate="fadeInLeft bounce">Información de los docentes</h3>
                </div>

                <div class="page-content-wrap ">
                    <div class="divider"></div>

                    <!-- page content holder -->
                    <div class="page-content-holder">

                        <div class="row">
                            <div class=" this-animate" data-animate="fadeInRight bounce">


                                <div class="col-md-6">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <div class="row">
                                                <div class=" col-md-12 titulo_alumnos">Docentes según su tipo de vinculación</div>
                                                <div class="row">

                                                    <div class="col-md-12">
                                                        <div class="col-md-3 parrafo_ficha">  <span class="numero_ficha col-md-12">@Html.DisplayFor(m => m.Institucionalidad.Voluntarios) </span>Voluntarios</div>
                                                        <div class="col-md-3 parrafo_ficha">  <span class="numero_ficha col-md-12 ">@Html.DisplayFor(m => m.Institucionalidad.ContratoLaboral) </span>Contrato laboral (Nómina)</div>
                                                        <div class="col-md-3 parrafo_ficha">
                                                            <span class="numero_ficha col-md-12">@Html.DisplayFor(m => m.Institucionalidad.ContratoHonorario)</span>Contrato por honorarios
                                                        </div>
                                                        <div class="col-md-3 parrafo_ficha">  <span class="numero_ficha col-md-12">@Html.DisplayFor(m => m.Institucionalidad.OrdenPrestacionServicios) </span>Orden de prestación de servicios</div>
                                                    </div>

                                                </div>
                                                <div class=" col-md-12 tabla_gris">Total Docentes @Html.DisplayFor(m => m.Institucionalidad.TotalDocentesVinculados)</div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="modal-content">

                                        <div class="modal-header">
                                            <div class="row">

                                                <div class=" col-md-12 titulo_alumnos2">
                                                    Docentes de acuerdo a su último nivel educativo

                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12 tabla_gris">

                                                        <div class="col-md-8 col-sm-8 col-xs-8 txt_fichas">
                                                            Primaria
                                                        </div>
                                                        <div class="col-md-4 col-sm-4 col-xs-4">
                                                            @Html.DisplayFor(m => m.Institucionalidad.Primaria)
                                                        </div>

                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12 tabla_blanca">

                                                        <div class="col-md-8 col-sm-8 col-xs-8 txt_fichas">
                                                            Secundaria
                                                        </div>
                                                        <div class="col-md-4 col-sm-4 col-xs-4">
                                                            @Html.DisplayFor(m => m.Institucionalidad.Secundaria)
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12 tabla_gris">

                                                        <div class="col-md-8 col-sm-8 col-xs-8 txt_fichas">
                                                            Técnico – tecnólogo
                                                        </div>
                                                        <div class="col-md-4 col-sm-4 col-xs-4">
                                                            @Html.DisplayFor(m => m.Institucionalidad.Tecnico)
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
