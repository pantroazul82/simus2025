@model WebSImus.Models.AgrupacionNuevoModels

<div class="panel panel-default">
    <div class="panel-heading">
        <h2><span class="fa-representante fa-2x"></span> Información Integrantes</h2>
    </div>
    <div class="panel-body">
        <div class="row push-down-20">
            <h3><span class="fa-representante fa-2x"></span> Datos de miembros de la agrupación</h3>
        </div>
        <div class="row push-down-20">
            <div class="row">
                <div class="col-md-12">
                    <p class="text-info"><i class="fa-question"></i> Para incluir cada integrantes de la agrupación debe haberse registrado como Agente y estar aprobado.</p>
                </div>
            </div>
            <div class="col-md-6">
               
                <div class="form-group">
                    <label class="col-md-4 control-label">Número de Identificación </label>
                    <div class="col-md-8">
                        @Html.Editor("Persona", new { htmlAttributes = new { @class = "form-control" } })
                    </div>
                </div>
            </div>
            <div class="col-md-6">

                <div class="form-group">

                    <div class="col-md-6">
                        <button type="button" id="mySubmitServ" class="btn btn-success glyphicon glyphicon-plus-sign" data-dismiss="modal"> Agregar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="TablaAgente" class="panel-body">
        @Html.Action("CargarAgentes", new { agrupacionId = Model.AgrupacionId, EliminarId = 0 })
    </div>
</div>
<script type="text/javascript">

    function EliminarAgente(varid) {
        var varAgenteId = '@Model.AgrupacionId';
        ActualizarListadoAgente(varAgenteId, varid)
    }

   
</script>

