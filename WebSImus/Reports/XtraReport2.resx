  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT distinct BAS_ZONAS_GEOGRAFICAS.ZON_ID as CODIGOMUNICIPIO,
		   BAS_ZONAS_GEOGRAFICAS.ZON_PADRE_ID AS CODIGODEPARTAMENTO,
		   ART_ENTIDADES_ARTES.ENT_ID,
		   dbo.FN_DEPARTAMENTOS(BAS_ZONAS_GEOGRAFICAS.ZON_ID) AS DEPARTAMENTO, 
		   BAS_ZONAS_GEOGRAFICAS.ZON_NOMBRE AS MUNICIPIO,
		   ART_ENTIDADES_ARTES.ENT_NOMBRE as Nombre,
		  CASE ART_ENTIDADES_ARTES.ENT_ESTADO WHEN 'E' THEN 'PUBLICADO' WHEN 'N' THEN 'NO PUBLICADO' ELSE '' END AS ESTADO, 
	      ISNULL (ART_MUSICA_CATEGORIA_ESCUELA.DESCRIPCION_CATEGORIA, '') AS CATEGORÍA, 
          ISNULL (ART_MUSICA_ENTIDAD_CATEGORIZACION.PORCENTAJE_TOTAL, 0) AS PORCENTAJE, 
          ART_ENTIDADES_ARTES.ENT_FECHA_ACTUALIZACION AS FECHA_ACTUALIZACIÓN,
		   (select ART_TIPOS_ESCUELAS.Nombre from ART_TIPOS_ESCUELAS where ART_TIPOS_ESCUELAS.Id = ART_MUSICA_ENTIDAD_IDENTIFICACION.ENT_TIPO_ESCUELA  ) AS TipoEscuela
	FROM ART_ENTIDADES_ARTES
	LEFT OUTER JOIN ART_MUSICA_ENTIDAD_CATEGORIZACION ON ART_ENTIDADES_ARTES.ENT_ID = ART_MUSICA_ENTIDAD_CATEGORIZACION.ENT_ID
	LEFT OUTER JOIN ART_MUSICA_CATEGORIA_ESCUELA ON ART_MUSICA_ENTIDAD_CATEGORIZACION.ID_CATEGORIA = ART_MUSICA_CATEGORIA_ESCUELA.ID_CATEGORIA
	INNER JOIN ART_ENTIDAD_UBICACION ON  ART_ENTIDADES_ARTES.ENT_ID = ART_ENTIDAD_UBICACION.ENT_ID		
	INNER JOIN BAS_ZONAS_GEOGRAFICAS ON BAS_ZONAS_GEOGRAFICAS.ZON_ID = ART_ENTIDAD_UBICACION.ZON_ID 
   	INNER JOIN ART_MUSICA_ENTIDAD_IDENTIFICACION ON  ART_ENTIDADES_ARTES.ENT_ID = ART_MUSICA_ENTIDAD_IDENTIFICACION.ENT_ID	
	INNER JOIN ART_ME_VIDEOS_ESCUELAS ON  ART_ENTIDADES_ARTES.ENT_ID = ART_ME_VIDEOS_ESCUELAS.ENT_ID		  	                   	
WHERE  ART_ENTIDADES_ARTES.ENT_TIPO = 'E'
order by ART_ENTIDADES_ARTES.ENT_NOMBRE</value>
  </data>
  <data name="customSqlQuery2.Sql" xml:space="preserve">
    <value>select ART_ME_VIDEOS_ESCUELAS.ENT_ID,
  CASE ART_ME_VIDEOS_ESCUELAS.VID_CLASIFICACION WHEN '1' THEN 'Proceso Formativo' 
  WHEN '2' THEN 'Encuentros' 
  WHEN '3' THEN 'Festivales' 
  WHEN '4' THEN 'Presentaciones públicas' 
  WHEN '5' THEN 'Ensayos' 
  WHEN '7' THEN 'Infraestructura' 
  WHEN '8' THEN 'Agrupaciones' 
  WHEN '9' THEN 'Recolección de fondos' 
  WHEN '10' THEN 'Intercambios musicales' 
  WHEN '11' THEN 'Jornadas de esparcimiento' 
  WHEN '12' THEN 'Vídeo institucional' 
  ELSE '' END AS CLASIFICACION,
ART_ME_VIDEOS_ESCUELAS.VID_CLASIFICACION,
ART_ME_VIDEOS_ESCUELAS.VID_DESCRIPCION,
ART_ME_VIDEOS_ESCUELAS.VID_ID,
ART_ME_VIDEOS_ESCUELAS.VID_FECHA_PUBLICACION,
ART_ME_VIDEOS_ESCUELAS.VID_URL 
from ART_ME_VIDEOS_ESCUELAS
order by ART_ME_VIDEOS_ESCUELAS.VID_FECHA_PUBLICACION</value>
